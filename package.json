{
  "name": "pscode",
  "displayName": "Common Pseudo Code Syntax Highlighter",
  "description": "Common Pseudo Code",
  "version": "0.0.5",
  "publisher": "adarshrkumar",
  "scripts": {
    "test": "echo \"Test Succesfull!\"",
    "publish": "vsce package && vsce publish minor",
    "next-version": "node nextVersion.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adarshrkumar/pscode.git"
  },
  "bugs": {
    "url": "https://github.com/adarshrkumar/pscode/issues"
  },
  "homepage": "https://github.com/adarshrkumar/pscode#readme",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pscode",
        "aliases": [
          "Common Pseudo Code",
          "pscode"
        ],
        "extensions": [
          ".pscode",
          ".pcode",
          ".pseudo",
          ".commonps"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pscode",
        "scopeName": "source.pscode",
        "path": "./syntaxes/pscode.tmLanguage.json"
      }
    ]
  }
}
